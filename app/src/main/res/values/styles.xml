<resources>

    <!-- Base application theme. -->
    <style name="ApplicationTheme" parent="Theme.AppCompat.Light.DarkActionBar">

        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="android:icon">@drawable/ic_bird_singing_with_musical_notes</item>

    </style>

    <style name="ButtonToggle">
        <item name="android:background">@drawable/toggle_drawable_layer</item>
        <item name="android:disabledAlpha">?android:attr/disabledAlpha</item>
        <item name="android:textOn">@null</item>
        <item name="android:textOff">@null</item>
    </style>

</resources>
